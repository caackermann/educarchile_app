<p id="notice"><%= notice %></p>

<h1>Project Choices</h1>

<table>
  <thead>
    <tr>
      <th>Desicion</th>
      <th>P1</th>
      <th>P2</th>
      <th>P3</th>
      <th>Project</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @project_choices.each do |project_choice| %>
      <tr>
        <td><%= project_choice.desicion %></td>
        <td><%= project_choice.p1 %></td>
        <td><%= project_choice.p2 %></td>
        <td><%= project_choice.p3 %></td>
        <td><%= project_choice.project %></td>
        <td><%= link_to 'Show', project_choice %></td>
        <td><%= link_to 'Edit', edit_project_choice_path(project_choice) %></td>
        <td><%= link_to 'Destroy', project_choice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Project Choice', new_project_choice_path %>
